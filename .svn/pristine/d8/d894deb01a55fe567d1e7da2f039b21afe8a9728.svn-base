<%@ include file="/common/taglibs.jsp"%>

<h3>OTHER APPLICATION INFORMATION</h3>
<s:hidden name="otherAppDetailsId" value="%{otherAppDetails.id}" /> 
<div>
    <div>
        <table class="inputform">
        <colgroup>
			<col width="200px" />
			<col />
		</colgroup>
            <tr>
                <td>Name of Sponsor: </td>
                <td><s:textfield name="otherAppDetails.nameOfSponsor" value="%{otherAppDetails.nameOfSponsor}"  /></td>
            </tr>
            <tr>
                <td>Address of Sponsor: </td>
                <td><s:textfield name="otherAppDetails.addressofSponsor" value="%{otherAppDetails.addressofSponsor}" /></td>
            </tr>
            <tr>
                <td>Estimated Start Date: *</td>
                <td><iita:datepicker name="otherAppDetails.startDate" value="%{otherAppDetails.startDate}" cssClass="datepicker" /></td>
            </tr>
            
        </table>
    </div>
</div>