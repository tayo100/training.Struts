		</action>
		
		<action name="application-list" class="cdoApplicationAction" method="applicationList">
			<result name="success">/WEB-INF/jsp/include/applicationlisting.jsp</result>
		</action>
		
		<action name="application-process" class="cdoApplicationProcessAction" method="execute">
			<result name="success">/WEB-INF/jsp/review/include/application-review.jsp</result>
		</action>
		
		<action name="cdointernalProcess" class="cdoApplicationProcessAction">
			<result name="success" type="redirect-action">application-process?applicationId=${applicationId}</result>
			<result name="error">/WEB-INF/jsp/review/include/application-review.jsp</result>
		</action>
		
		<action name="trainingproposaldelete" class="cdoTrainingProposalAction" method="delete">
			<result name="success" type="redirect-action">trainingproposal</result>
			<result name="error">/WEB-INF/jsp/announcement/cdotrainingproposal.jsp</result>
		</action>
		
		<action name="deleteannouncement" class="cdoAnnouncementAction" method="delete">
			<result name="success" type="redirect-action">cdoindex</result>
			<result name="error">/WEB-INF/jsp/announcement/cdoannouncement-detail.jsp</result>
		</action>
		
		<action name="save-proposal" class="cdoTrainingProposalAction" method="save">
			<result>/WEB-INF/jsp/announcement/cdotrainingproposal-form.jsp</result>
			<result name="error">/WEB-INF/jsp/announcement/cdotrainingproposal-form.jsp</result>
			<result name="input">/WEB-INF/jsp/announcement/cdotrainingproposal-form.jsp</result>
