			<result name="success">/WEB-INF/jsp/announcement/cdotrainingproposal.jsp</result>
			<result name="error">/WEB-INF/jsp/announcement/cdotrainingproposal.jsp</result>
			<result name="input">/WEB-INF/jsp/announcement/cdotrainingproposal.jsp</result>
		</action>
		
