		<action name="trainingproposaledit-document" class="trainingProposalDocumentAction">
			<result type="redirect-action">proposaleditdata?id=${entityDocument.entity.id}</result>
		</action>
		
		<action name="add-trainer" class="addTrainerAction">
			<result name="trainee" type="redirect-action">trainee/profile?id=${trainee.id}</result>
			<result name="program" type="redirect-action">program/profile?id=${trainingProgram.id}</result>
			<result name="trainingproposal" type="redirect-action">proposaleditdata?id=${trainingProposal.id}</result>
			<result name="announcement" type="redirect-action">profile?id=${announcement.id}</result>
		</action>
		
