<%@ include file="/common/taglibs.jsp"%>

<s:form action="" method="post">
	<s:hidden name="type" value="org.iita.trainingunit.model.IYARegistration" />
	<table class="inputform">
		<colgroup>
			<col width="20%" />
			<col width="30%" />
			<col />
		</colgroup>
		<tr>
			<td>First Name:</td>
			<td><s:textfield name="firstName" value="%{firstName}" /></td>
		</tr>
		<tr>
			<td>Last Name:</td>
			<td><s:textfield name="lastName" value="%{lastName}" /></td>
		</tr>
				<tr>
			<td>Date of Birth:</td>
			<td><s:date name="doBirth" format = "dd/mm/yyyy" value="%{doBirth}" /></td>
		</tr>
		<tr>
			<td>Gender:</td>
			<td><s:textfield name="gender" value="%{gender}" /></td>
		</tr>
		<tr>
			<td>Phone No:</td>
			<td><s:textfield name="phoneNo" value="%{phoneNo}" /></td>
		</tr>
		<tr>
			<td>Email:</td>
			<td><s:textfield name="email" value="%{email}" /></td>
		</tr>
		<tr>
			<td>Qualification:</td>
			<td><s:textfield name="qualification" value="%{qualification}" /></td>
		</tr>
		<tr>
			<td>Address:</td>
			<td><s:textfield name="address" value="%{address}" /></td>
		</tr>
				<tr>
			<td>State:</td>
			<td><s:textfield name="state" value="%{state}" /></td>
		</tr>
		<tr>
			<td></td>
			<td><s:submit value="Submit" /></td>
		</tr>
	</table>
</s:form>