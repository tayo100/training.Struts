<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- Application mail package -->
	<package name="appmail" extends="public">
		<action name="appmail/index" class="listMailAction">
			<result>/WEB-INF/jsp/appmail/list.jsp</result>
		</action>
		<action name="appmail/mail" class="mailAction">
			<result>/WEB-INF/jsp/appmail/mail.jsp</result>
		</action>
	</package>
</struts>