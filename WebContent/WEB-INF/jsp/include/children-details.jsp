<%@ include file="/common/taglibs.jsp"%>

<h3>APPLICANT'S CHILDREN</h3>
<div>
	<div>
	    <table class="inputform" id="childrenTable">
	    <colgroup>
			<col width="200px" />
			<col width="300px" />
			<col />
		</colgroup>
		<tr>
			<td />
			<td>Child's Names:</td>
			<td>Date of Birth:</td>
		</tr>
		<s:iterator value="biodata.applicantsChildren" status="status">
			<tr>
				<td />
				<td><s:property value="childName" /></td>
                <td><iita:date name="dateOfBirth" format="dd/MM/yyyy" /></td>
            </tr>
		</s:iterator>
	    </table>
	</div>
</div>