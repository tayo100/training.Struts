<%@ include file="/common/taglibs.jsp"%>

<s:if test="otherAppDetails.size()>0">
	<h3>OTHER APPLICATION INFORMATION</h3>
	<s:hidden name="otherAppDetailsId" value="%{otherAppDetails.id}" /> 
	<div>
	    <div>
	        <table class="inputform">
	        <colgroup>
				<col width="200px" />
				<col />
			</colgroup>
	            <tr>
	                <td>Name of Sponsor: </td>
	                <td><s:property value="otherAppDetails.nameOfSponsor"  /></td>
	            </tr>
	            <tr>
	                <td>Address of Sponsor: </td>
	                <td><s:property value="otherAppDetails.addressofSponsor" /></td>
	            </tr>
	            <tr>
	                <td>Estimated Start Date: *</td>
	                <td><s:property value="otherAppDetails.startDate" /></td>
	            </tr>
	            
	        </table>
	    </div>
	</div>
</s:if>