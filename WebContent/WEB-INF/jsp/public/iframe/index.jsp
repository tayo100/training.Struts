<%@ include file="/common/taglibs.jsp"%>
<html>
<head>
<title>CDO/Projects Training Announcements</title>
</head>
<body>

<s:include value="/WEB-INF/jsp/include/iframe-announcementlisting.jsp" />

<div class="container">
	<s:form cssClass="form-horizontal" method="post" namespace="/application" action="register">
		<fieldset>
		
		<!-- Form Name -->
		<legend>Training Application</legend>
		
		<!-- Select Basic -->
		<div class="form-group">
		  <label class="col-md-2 control-label" for="trainingOption">Select Type:</label>
		  <div class="col-md-10">
		    <select id="trainingOption" name="trainingOption" class="form-control">
		    	<option value="">--Training type--</option>
		      <option value="Graduate">Graduate Trainee</option>
		      <option value="Group">Group</option>
		      <option value="Internship">Internship</option>
		      <option value="Sabbatical">Sabbatical</option>
		    </select>
		  </div>
		</div>
		
		<!-- Button -->
		<div class="form-group">
		  <label class="col-md-2 control-label" for="singlebutton"></label>
		  <div class="col-md-10">
		    <button type="submit" id="singlebutton" name="singlebutton" class="btn btn-info">Start Application</button>
		  </div>
		</div>
		
		</fieldset>
	</s:form>
</div>
</body>
</html>